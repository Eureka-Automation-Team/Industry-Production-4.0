@section Scripts{
    <script src="~/plugins/datepicker/datepicker.min.js"></script>

    <!-- jsGrid -->
    <script src="~/plugins/jsgrid/demos/db.js"></script>
    <script src="~/plugins/jsgrid/jsgrid.min.js"></script>

    <script>
        //Date range picker
        $('#requireDateFrom').datepicker({ dateFormat: 'dd/mm/yy', isBuddhist: false, defaultDate: Date() });
        $('#requireDateTo').datepicker({ dateFormat: 'dd/mm/yy', isBuddhist: false, defaultDate: Date() });

        $(function () {
            setInterval(
                function () {
                    var startDate = $("#requireDateFrom").val();
                    var endDate = $("#requireDateTo").val();
                    var machineNo = $("#searchMachineNo").val();
                    var machineGroup = $("#searchMachineGroup").val();
                    var machineModel = $("#inputModel").val();
                    var prodLine = $("#prodLine").val();

                    $("#jsGrid1").jsGrid({
                        //height: "100%",
                        width: "100%",

                        sorting: true,
                        paging: true,
                        autoload: true,
                        pageSize: 3,

                        controller: {
                            loadData: function (filter) {
                                return $.ajax({
                                    type: "GET",
                                    url: "/Production/GetTaskQueing",
                                    data: "startDate=" + startDate + "&endDate=" + endDate + "&machineNo=" + machineNo + "&machineGroup=" + machineGroup + "&machineModel=" + machineModel + "&pdLine=" + prodLine,
                                    dataType: "JSON",
                                    success: function (response) {
                                        //console.log(response);
                                    }
                                }).done(function (response) {
                                    //console.log(machineNo);
                                    //console.log(endDate);
                                })
                            }
                        },
                        fields: [
                            { name: "Priority", type: "number", width: 30, align: "center", title: "Seq." },
                            { name: "JobEntity.PrimaryItemCode", type: "text", align: "center", title: "Part Number" },
                            { name: "TableNumber", type: "text", width: 50, align: "center", title: "Table No." },
                            { name: "JobNumber", type: "text", width: 50, align: "center", title: "Job No." },
                            { name: "DueDate", type: "text", align: "center", title: "Required Date" },
                            { name: "MachineNoReady", type: "text", align: "center", title: "Machine Code" },
                            { name: "", type: "text", align: "center", title: "PD Status" }
                        ]
                    });
                    //console.log("test");
                }
                , 2000); // every 3 sec
        });

        $(function () {
            setInterval(
                function () {
                    var startDate = $("#requireDateFrom").val();
                    var endDate = $("#requireDateTo").val();
                    var machineNo = $("#searchMachineNo").val();
                    var machineGroup = $("#searchMachineGroup").val();
                    var machineModel = $("#inputModel").val();
                    var prodLine = $("#prodLine").val();
                    var url = "/Production/GetTaskSummaries";

                    $.getJSON(url, { startDate: startDate, endDate: endDate, machineNo: machineNo, machineGroup: machineGroup, machineModel: machineModel, pdLine: prodLine },
                        function (data) {
                            var plan = "<h3>" + data.planCount + "</h3>";
                            var actual = "<h3>" + data.actualCount + "</h3>";
                            var complated = "<h3>" + data.completeCount.toFixed(0) + " %</h3>";

                            console.log(data.timeProgress);

                            var timeProgress = $("<div>");
                            timeProgress.addClass("progress-bar bg-primary progress-bar-striped");
                            timeProgress.attr("role", "progressbar");
                            timeProgress.attr("aria-valuenow", data.timeProgress);
                            timeProgress.attr("aria-valuemin", 0);
                            timeProgress.attr("aria-valuemax", 100);
                            timeProgress.attr("style", "width: " + data.timeProgress + "%");

                            var prodProgress = $("<div>");
                            prodProgress.addClass("progress-bar bg-success progress-bar-striped");
                            prodProgress.attr("role", "progressbar");
                            prodProgress.attr("aria-valuenow", data.productionProgress);
                            prodProgress.attr("aria-valuemin", 0);
                            prodProgress.attr("aria-valuemax", 100);
                            prodProgress.attr("style", "width: " + data.productionProgress + "%");

                            var planProgress = $("<div>");
                            planProgress.addClass("progress-bar bg-warning progress-bar-striped");
                            planProgress.attr("role", "progressbar");
                            planProgress.attr("aria-valuenow", data.planProgress);
                            planProgress.attr("aria-valuemin", 0);
                            planProgress.attr("aria-valuemax", 100);
                            planProgress.attr("style", "width: " + data.planProgress + "%");
                            
                            //$.each(data, function (i, courselist) {
                            //    vals += "<h3>" + data[i].PlanCount + "</h3>"
                            //})
                            $("#planCount").html(plan);
                            $("#actualCount").html(actual);
                            $("#completeCount").html(complated);
                            $("#timeProgress").html(timeProgress);
                            $("#productionProgress").html(prodProgress);
